{
    "manifest_version": 3,
    "name": "Timer",
    "version": "1.0",
    "action": {
        "default_popup": "/html/popup.html",
        "default_title": "View timer"
    },
    "author": "Vaishnav Dixit",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["/css/style.css"]
        }
    ],
    "description": "Set a quick timer in your extension"
}